{
  "expo": {
    "name": "BrighterSpace",
    "slug": "brigherspace",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/Logo/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Logo/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFD79F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "package": "com.brigherspace.brigherspace"
    },
    "android": {
      "useNextNotificationsApi": true,
      "permissions": ["android.permission.SCHEDULE_EXACT_ALARM", "RECEIVE_BOOT_COMPLETED"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo/logo.png",
        "backgroundColor": "#FFD79F"
      },
      "package": "com.brigherspace.brigherspace"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "55336044-565c-49e0-bcf2-480270967740"
      }
    },
    "owner": "notaymanm"
  }
}
